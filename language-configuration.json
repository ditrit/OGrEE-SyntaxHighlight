{
	"comments": {
	  "lineComment": "//"
	},
	"brackets": [
	  ["[", "]"]
	],
	"autoClosingPairs": [
	  { "open": "[", "close": "]" }
	],
	"autoCloseBefore": ";:.,=}])>` \n\t",
	"surroundingPairs": [
	  ["[", "]"]
	],
	"wordPattern": "(-?\\d*\\.\\d\\w*)|([^\\`\\~\\!\\@\\#\\%\\^\\&\\*\\(\\)\\-\\=\\+\\[\\{\\]\\}\\\\\\|\\;\\:\\'\\\"\\,\\.\\<\\>\\/\\?\\s]+)",
	"indentationRules": {
	  "increaseIndentPattern": "^((?!\\/\\/).)*(\\{[^}\"'`]*|\\([^)\"'`]*|\\[[^\\]\"'`]*)$",
	  "decreaseIndentPattern": "^((?!.*?\\/\\*).*\\*/)?\\s*[\\)\\}\\]].*$"
	}
  }