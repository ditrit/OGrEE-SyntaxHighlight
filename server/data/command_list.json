{
    "ignored": [
        "ls",
        "get",
        "getu",
        "getslot",
        "undraw",
        "draw",
        "drawable",
        "unset",
        "env",
        "+",
        "-",
        "=",
        "len",
        "link",
        "unlink",
        "print",
        "printf",
        "man",
        "cd",
        "tree",
        "ui.",
        "camera.",
        ">",
        "while",
        "for",
        "if",
        "alias"
    ],
    "commands": [
        {
            "keyword": ".var",
            "matchKeyword" : ["var"],
            "quickText": "Declare variable",
            "documentation": "",
            "insertText": ".var:${1:name}=${0:value}"
        },
        {
            "keyword": ".cmds",
            "matchKeyword" : ["cmds"],
            "quickText": "Load commands from file",
            "documentation": "[path] path of the file",
            "insertText": ".cmds:${0:path}"
        }
        ,
        {
            "keyword": "template",
            "matchKeyword" : ["template"],
            "quickText": "Load template from JSON",
            "documentation": "[path] path of the file",
            "insertText": ".template:${0:path}"
        }
        ,
        {
            "keyword": "site",
            "matchKeyword" : ["site", "st"],
            "quickText": "Create a Site",
            "documentation": "Site must be child of a Tenant",
            "insertText": "+site:${0:name}"
        }
        ,
        {
            "keyword": "rack",
            "matchKeyword" : ["rack", "rk"],
            "quickText": "Create a Rack",
            "documentation": "",
            "insertText": "+rack:${1:name}@$[${2:x}, ${3:y}]@${4|m,t,f|}@${5|[x y z],front,rear,left,right,top,bottom|}@[${6:width}, ${7:length}, ${0:height}]"
        }
        ,
        {
            "keyword": "tenant",
            "matchKeyword" : ["tenant", "tn"],
            "quickText": "Create a Tenant",
            "documentation": "Tenant will be created as a new root",
            "insertText": "+rack:${1:name}@${0:color}"
        }
        ,
        {
            "keyword": "building",
            "matchKeyword" : ["building", "bd"],
            "quickText": "Create a Building",
            "documentation": "Building must be child of a Site.\n [pos] is a Vector2 [x,y] (m,m)\n [rotation] is the rotation of the building around its lower left corner, in degree\n [size] is a Vector3 [width,length,height] (m,m,m)\n [template] is the name (slug) of the building template",
            "insertText": "+building:${1:name}@${2:pos}@${3:rotation}@[${4:width}, ${5:length}, ${6:height}]@${0:template}"
        }
        ,
        {
            "keyword": "rack",
            "matchKeyword" : ["rack", "rk"],
            "quickText": "Create a Rack",
            "documentation": "",
            "insertText": "+rack:${1:name}@$[${2:x}, ${3:y}]@${4|m,t,f|}@${5|[x y z],front,rear,left,right,top,bottom|}@[${6:width}, ${7:length}, ${0:height}]"
        }
        ,
        {
            "keyword": "room",
            "matchKeyword" : ["room", "ro"],
            "quickText": "Create a Room",
            "documentation": "Room must be child of a building.\nIts name will be displayed in the center of the room in its local coordinates system.\n[pos] is a Vector2 [x,y] (m,m)\n[rotation] is the rotation of the building around its lower left corner, in degree\n[size] is a Vector3 [width,length,height] (m,m,m)\n[axisOrientation] defines the orientation of the rows and columns. It can be any combinason of [+/-]x[+/-]y. eg: +x+y or -x+y\n[template] is the name of the room template\n[floorUnit] is optionnal: by default set to <<t>> (tiles), can also be m (meters) or f (feet)",
            "insertText": "+room:${1:name}@[${2:x},${3:y}]@${4:rotation}@$[${5:width}, ${6:length}, ${7:height}]@${8|+x,+y,-x,-y,+x+y,+x-y,-x,+y,-x-y|}@${0:floorUnit}"
        }
    ],
    "objects": [
        "domain",
        "dm",
        "si",
        "bldg",
        "rk",
        "device",
        "dv",
        "corridor",
        "co",
        "group",
