{
	"name": "OGrEE Default color theme",
	"type": "dark",
	"semanticHighlighting": true,
	"tokenColors": [
		{
			"scope": [
				"unknown"
			],
			"settings": {
				"foreground": "#ff1900"
			}
		},
		{
			"scope": [
				"comment",
				"comment.block.documentation"
			],
			"settings": {
				"foreground": "#6A9955"
			}
		},
		{
			"scope": [
				"string",
				"string.quoted.single",
				"string.quoted.double"
			],
			"settings": {
				"foreground": "#ff00ff"
			}
		},
		{
			"scope": [
				"entity.name.variable"
			],
			"settings": {
				"foreground": "#ff0000"
			}
		},		
		{
			"scope": [
				"entity.name.type.enum"
			],
			"settings": {
				"foreground": "#ffff00"
			}
		},
		{
			"scope": [
				"entity.name.operator"
			],
			"settings": {
				"foreground": "#ffff00"
			}
		},
		{
			"scope": [
				"*.deprecated"
			],
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"entity.name.bebou"
			],
			"settings": {
				"foreground": "#00ff00",
				"fontStyle":"underline",
				"background" : "ff0000"
			}
		},
		{
			"scope": [
				"entity.name.function.macro"
			],
			"settings": {
				"foreground": "#000000"
			}
		}
	]
}